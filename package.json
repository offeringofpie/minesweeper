{
  "name": "Minesweeper",
  "version": "0.0.1",
  "description": "Minesweeper in canvas",
  "author": "J Lopes",
  "repository": "https://github.com/offeringofpie/minesweeper",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --mode development --hot --inline",
    "clean": "rimraf docs",
    "build": "npm run clean && webpack --mode production"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "postCSS": {
    "parser": "sugarss",
    "plugins": {
      "autoprefixer": {
        "browsers": [
          "last 3 versions"
        ]
      },
      "postcss-import": {},
      "postcss-cssnext": {
        "browsers": [
          "last 3 versions"
        ]
      },
      "cssnano": {}
    }
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "clipboardy": "^1.2.3",
    "css-loader": "^0.28.11",
    "css-mqpacker": "^6.0.2",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "postcss-color-function": "^4.0.1",
    "postcss-custom-media": "^7.0.7",
    "postcss-extend": "^1.0.5",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.6",
    "postcss-preset-env": "^6.5.0",
    "rimraf": "^2.6.2",
    "rxjs": "^5.5.11",
    "style-loader": "^0.19.1",
    "terser-webpack-plugin": "^1.2.1",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14",
    "webpack-notifier": "^1.6.0",
    "webpackbar": "^3.1.4",
    "wepack": "0.0.1"
  }
}
