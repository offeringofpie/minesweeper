{
  "name": "Minesweeper",
  "version": "0.0.1",
  "description": "Minesweeper in canvas",
  "author": "J Lopes",
  "repository": "https://github.com/offeringofpie/minesweeper",
  "license": "MIT",
  "scripts": {
    "start": "webpack-serve --config ./webpack.config.js",
    "build": "webpack --mode production"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "postCSS": {
    "parser": "sugarss",
    "plugins": {
      "autoprefixer": {
        "browsers": [
          "last 3 versions"
        ]
      },
      "postcss-import": {},
      "postcss-cssnext": {
        "browsers": [
          "last 3 versions"
        ]
      },
      "cssnano": {}
    }
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^0.28.11",
    "css-mqpacker": "^6.0.2",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.6",
    "rxjs": "^5.5.11",
    "style-loader": "^0.19.1",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-hot-client": "^4.1.1",
    "webpack-notifier": "^1.6.0",
    "webpack-serve": "^1.0.4"
  }
}
